{
  "name": "eslint-plugin-react-email",
  "version": "0.0.9",
  "description": "Lint your emails for things that might be unsupported on different email clinets",
  "sideEffects": false,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./rules/no-accent-color": "./dist/no-accent-color.js",
    "./rules/no-animation": "./dist/no-animation.js",
    "./rules/no-aspect-ratio": "./dist/no-aspect-ratio.js",
    "./rules/no-border-inline-block-logicals": "./dist/no-border-inline-block-logicals.js",
    "./rules/no-border-inline-block-longhand": "./dist/no-border-inline-block-longhand.js",
    "./rules/no-border-inline-block": "./dist/no-border-inline-block.js",
    "./rules/no-border-radius-logicals": "./dist/no-border-radius-logicals.js",
    "./rules/no-box-shadow": "./dist/no-box-shadow.js",
    "./rules/no-base64-images": "./dist/no-base64-images.js",
    "./rules/no-clip-path": "./dist/no-clip-path.js",
    "./rules/no-column-layout-properties": "./dist/no-column-layout-properties.js",
    "./rules/no-conic-gradient": "./dist/no-conic-gradient.js",
    "./rules/no-flex-wrap": "./dist/no-flex-wrap.js",
    "./rules/no-function-clamp": "./dist/no-function-clamp.js",
    "./rules/no-function-max": "./dist/no-function-max.js",
    "./rules/no-function-min": "./dist/no-function-min.js",
    "./rules/no-gap": "./dist/no-gap.js",
    "./rules/no-grid-template": "./dist/no-grid-template.js",
    "./rules/no-hyphens": "./dist/no-hyphens.js",
    "./rules/no-important": "./dist/no-important.js",
    "./rules/no-block-inline-size": "./dist/no-block-inline-size.js",
    "./rules/no-intrinsic-size": "./dist/no-intrinsic-size.js",
    "./rules/no-margin-block-start-end": "./dist/no-margin-block-start-end.js",
    "./rules/no-margin-inline-block": "./dist/no-margin-inline-block.js",
    "./rules/no-margin-inline-start-end": "./dist/no-margin-inline-start-end.js",
    "./rules/no-max-block-size": "./dist/no-max-block-size.js",
    "./rules/no-min-inline-size": "./dist/no-min-inline-size.js",
    "./rules/no-modern-color": "./dist/no-modern-color.js",
    "./rules/no-outline-offset": "./dist/no-outline-offset.js",
    "./rules/no-overflow-wrap": "./dist/no-overflow-wrap.js",
    "./rules/no-padding-block-start-end": "./dist/no-padding-block-start-end.js",
    "./rules/no-padding-inline-block": "./dist/no-padding-inline-block.js",
    "./rules/no-padding-inline-start-end": "./dist/no-padding-inline-start-end.js",
    "./rules/no-position": "./dist/no-position.js",
    "./rules/no-scroll-snap": "./dist/no-scroll-snap.js",
    "./rules/no-tab-size": "./dist/no-tab-size.js",
    "./rules/no-text-align-last": "./dist/no-text-align-last.js",
    "./rules/no-text-decoration-thickness": "./dist/no-text-decoration-thickness.js",
    "./rules/no-text-emphasis-position": "./dist/no-text-emphasis-position.js",
    "./rules/no-text-emphasis": "./dist/no-text-emphasis.js",
    "./rules/no-text-underline-offset": "./dist/no-text-underline-offset.js",
    "./rules/no-word-break": "./dist/no-word-break.js"
  },
  "files": [
    "dist/**",
    "caniemail/**"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rollup --config=rollup.config.js --configPlugin=rollup-plugin-esbuild",
    "dev": "pnpm build --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/resendlabs/react-email.git",
    "directory": "packages/eslint-plugin"
  },
  "keywords": [
    "react",
    "email"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "jsx-ast-utils": "^3.3.5",
    "parse-md": "^3.0.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-react": "7.22.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/eslint": "^8.44.7",
    "@types/html-to-text": "9.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/pretty": "2.0.1",
    "@types/react": "18.2.33",
    "@typescript-eslint/utils": "^6.11.0",
    "eslint-config-custom": "workspace:*",
    "rollup": "^4.4.1",
    "rollup-plugin-esbuild": "^6.1.0",
    "tsconfig": "workspace:*",
    "typescript": "5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
